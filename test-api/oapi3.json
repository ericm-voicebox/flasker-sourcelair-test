{
  "openapi": "3.0.0",
  "info": {
    "title": "test-api",
    "description": "",
    "version": "1.0.0"
  },
  "paths": {
    "/etl/:appId/jobs/:importId/": {
      "get": {
        "summary": "Get the validation status of a particular ETL job.",
        "description": "Returns the details of a data ingestion job associated with the provided application Id and import Id.\n",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemasValidationResponseList"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "ValidationResponseList": {
        "properties": {
          "validations": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ValidationResponse"
            }
          }
        }
      },
      "ValidationResponse": {
        "title": "Root Type for ValidationResponse",
        "description": "The root of the ValidationResponse type's schema.",
        "type": "object",
        "properties": {
          "type": {
            "description": "The type of validation that was run",
            "type": "string"
          },
          "status": {
            "description": "The status of the validation",
            "type": "string"
          },
          "message": {
            "description": "Developer message describing the validation response status.",
            "type": "string"
          }
        }
      }
    }
  }
}